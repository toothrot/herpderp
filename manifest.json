{
  "manifest_version": 2,

  "name": "More Herpderp",
  "description": "",
  "version": "3.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://www.youtube.com/",
    "http://www.youtube.com/"
  ],
  "content_scripts": [
  {
    "matches": ["http://www.youtube.com/*"],
    "js": ["herpderp.js"]
  }
],
}
